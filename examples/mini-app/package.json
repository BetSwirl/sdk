    "postcss-import": "^16.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.1.0",
    "storybook": "8.2.9",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "^4.1.7",
    "tw-animate-css": "^1.3.0",
    "typescript": "~5.7.3",
    "viem": "2.29.1",
    "vite": "^6.3.5",
    "vitest": "^3.1.4",
    "wagmi": "^2.15.4"
  },
  "loki": {
    "configurations": {
      "chrome.laptop": {
        "target": "chrome.docker",
        "width": 1366,
        "height": 768,
        "deviceScaleFactor": 1,
        "mobile": false
      },
      "chrome.iphone7": {
        "target": "chrome.docker",
        "preset": "iPhone 7"
      }
    }
  }
}
