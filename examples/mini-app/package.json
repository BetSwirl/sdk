{
  "name": "@betswirl/ui",
  "repository": {
    "type": "git",
    "url": "https://github.com/chainhackers/sdk.git",
    "directory": "examples/mini-app"
  },
  "version": "0.0.15",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./styles.css": "./dist/index.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "rm -rf dist && tsc -b && NODE_ENV=production pnpm exec tailwindcss -i ./src/index.css -o ./dist/index.css --postcss --minify && cp -r src/assets dist/ && vite build",
    "lint": "biome check .",
    "lint:fix": "biome check . --write",
    "format": "biome format . --write",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test:loki": "pnpm loki test --chromeTolerance 1",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "decimal.js": "^10.5.0",
    "lucide-react": "^0.503.0"
  },
  "peerDependencies": {
    "@betswirl/sdk-core": "^0.1.6",
    "@betswirl/wagmi-provider": "^0.1.6",
    "@coinbase/onchainkit": "^0.38.13",
    "@tanstack/react-query": "^5.77.2",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "viem": "2.29.1",
    "wagmi": "^2.15.4"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    },
    "react-dom": {
      "optional": false
    },
    "@betswirl/sdk-core": {
      "optional": false
    },
    "@betswirl/wagmi-provider": {
      "optional": false
    },
    "@coinbase/onchainkit": {
      "optional": false
    },
    "@tanstack/react-query": {
      "optional": false
    },
    "viem": {
      "optional": false
    },
    "wagmi": {
      "optional": false
    }
  },
  "devDependencies": {
    "@betswirl/sdk-core": "^0.1.6",
    "@betswirl/wagmi-provider": "^0.1.6",
    "@biomejs/biome": "^1.9.4",
    "@chromatic-com/storybook": "^3.2.6",
    "@coinbase/onchainkit": "^0.38.13",
    "@storybook/addon-essentials": "8.2.9",
    "@storybook/blocks": "8.2.9",
    "@storybook/react": "8.2.9",
    "@storybook/react-vite": "8.2.9",
    "@storybook/test": "8.2.9",
    "@tailwindcss/cli": "^4.1.10",
    "@tailwindcss/postcss": "^4.1.10",
    "@tailwindcss/vite": "^4.1.7",
    "@tanstack/react-query": "^5.77.2",
    "@types/node": "^22.15.21",
    "@types/react": "^19.1.5",
    "@types/react-dom": "^19.1.5",
    "@vitejs/plugin-react": "^4.5.0",
    "@vitest/browser": "^3.1.4",
    "@vitest/coverage-v8": "^3.1.4",
    "autoprefixer": "^10.4.21",
    "cssnano": "^7.0.7",
    "globals": "^16.2.0",
    "loki": "^0.35.1",
    "playwright": "^1.52.0",
